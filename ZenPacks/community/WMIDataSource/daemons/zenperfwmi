. $ZENHOME/bin/zenfunctions

MYPATH=`readlink -f $0`
THISDIR=`dirname $MYPATH`
PRGHOME=`dirname $THISDIR`
PRGNAME=zenperfwmi.py
CFGFILE=$CFGDIR/zenperfwmi.conf
if [ ! -f $CFGFILE ]; then
    touch $CFGFILE
fi

generic "$@"

